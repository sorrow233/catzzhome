{
    "name": "catzzhome",
    "version": "1.0.7",
    "description": "Catzz Homepage",
    "scripts": {
        "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
        "build:sitemap": "node scripts/generate-sitemap.js",
        "watch:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
        "deploy": "npm run build:css && npm run build:sitemap && npx wrangler pages deploy . --project-name catzzhome"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "tailwindcss": "^3.4.0"
    }
}